{
   "name": "pemilu_dumbways",
   "version": "1.0.0",
   "description": "A microservice (backend) for pemilu dumbways site using express ts and TypeORM",
   "main": "src/index.ts",
   "scripts": {
      "start": "nodemon --exec ts-node src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:generate": "npx typeorm-ts-node-commonjs migration:generate src/migrations/User -d ./src/data-source.ts",
      "migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/data-source.ts"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/rahmat-end/Dumbways_ST2_OL.git"
   },
   "keywords": [
      "Express",
      "Typescript",
      "TypeORM"
   ],
   "author": "Rahmat Kurniawan",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/rahmat-end/Dumbways_ST2_OL/issues"
   },
   "homepage": "https://github.com/rahmat-end/Dumbways_ST2_OL#readme",
   "dependencies": {
      "bcrypt": "^5.1.1",
      "cloudinary": "^1.41.0",
      "cors": "^2.8.5",
      "express": "^4.18.2",
      "express-fileupload": "^1.4.2",
      "fs-extra": "^11.1.1",
      "hbs": "^4.2.0",
      "joi": "^17.11.0",
      "jsonwebtoken": "^9.0.2",
      "multer": "^1.4.5-lts.1",
      "nodemon": "^3.0.1",
      "path": "^0.12.7",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.17"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.2",
      "@types/express": "^4.17.21",
      "@types/express-fileupload": "^1.4.4",
      "@types/jsonwebtoken": "^9.0.5",
      "@types/multer": "^1.4.10",
      "@types/node": "^16.11.10",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   }
}
